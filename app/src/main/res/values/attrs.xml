<resources>

  <!-- StrokedTextView specific attributes. -->
  <declare-styleable name="StrokedTextView">
    <!-- The color of the stroke outline -->
    <attr format="color" name="strokeColor" />
    <!-- The color of the text -->
    <attr format="color" name="strokeTextColor" />
    <!-- The width of the stroke -->
    <attr format="float" name="strokeWidth" />
  </declare-styleable>

  <declare-styleable name="LeftImageButton">
    <attr format="integer" name="image" />
    <attr format="dimension" name="imageWidth" />
    <attr format="dimension" name="imageHeight" />
    <attr format="string" name="label" />
    <attr name="android:textColor" />
    <attr name="android:textSize" />
    <attr name="android:scaleType" />
    <attr name="android:tint" />
  </declare-styleable>

</resources>

