          android:layout_alignEnd="@+id/register_panel2"
          android:layout_alignRight="@+id/register_panel2"
          android:layout_centerVertical="true"
          android:layout_gravity="center"
          android:layout_marginRight="10dp"
          android:background="@drawable/registro_forward_green_selector"
          android:clickable="true"
          android:onClick="goNext"
          />

