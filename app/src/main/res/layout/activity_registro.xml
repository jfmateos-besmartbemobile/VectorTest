<?xml version="1.0" encoding="utf-8"?>

<com.mobile.android.smartick.UI.RegisterScrollView
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/register_horizontalScroll"
    android:orientation="horizontal"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:animateLayoutChanges="true"
    android:background = "@drawable/intro_page6_bg_sky"
    android:scrollbars="none"
    android:overScrollMode="never"
    android:fillViewport="true">

    <LinearLayout
        android:id="@+id/register_horizontalScroll_linear"
        android:orientation="horizontal"
        android:layout_width="900dp"
        android:layout_height="450dp"
        android:layout_alignParentRight="true"
        android:fillViewport="true"
        >

        <RelativeLayout
            android:id="@+id/register_page1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            >

            <ImageView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:src="@drawable/register_panel"
                android:layout_gravity="center"
                android:id="@+id/register_panel1"
                android:scaleType="fitCenter"
                android:adjustViewBounds="true"
                android:layout_centerVertical="true"
                android:layout_centerHorizontal="true" />

            <TextView
                android:id="@+id/student_credentials_title"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textColor="@color/BlueColor"
                android:textSize="32dp"
                android:text="@string/Students_Data"
                android:layout_alignTop="@+id/register_panel1"
                android:layout_marginTop="20dp"
                android:layout_centerHorizontal="true" />

            <ImageView
                android:id="@+id/student_username_icon"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:background="@drawable/student_username_selector"
                android:layout_below="@+id/student_credentials_title"
                android:layout_toLeftOf="@+id/student_credentials_title"
                android:layout_toStartOf="@+id/student_credentials_title"
                android:layout_marginRight="24dp"
                android:layout_marginEnd="64dp"
                android:layout_marginTop="34dp"/>

            <ImageView
                android:id="@+id/student_password_icon"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:background="@drawable/student_password_selector"
                android:layout_below="@+id/student_username_icon"
                android:layout_alignLeft="@+id/student_username_icon"
                android:layout_alignStart="@+id/student_username_icon"
                android:layout_marginTop="36dp" />

            <EditText
                android:id="@+id/student_alias_edittext"
                android:layout_width="300dp"
                android:layout_height="80dp"
                android:background="@drawable/rounded_edittext_register"
                android:hint="@string/Choose_alias"
                android:paddingLeft="20dp"
                android:textSize="20dp"
                android:lines="1"
                android:inputType="textPersonName"
                android:textColor="@color/GrayColor"
                android:layout_alignTop="@+id/student_username_icon"
                android:layout_toRightOf="@+id/student_username_icon"
                android:layout_toEndOf="@+id/student_username_icon" />

            <EditText
                android:id="@+id/student_password_edittext"
                android:layout_width="300dp"
                android:layout_height="80dp"
                android:background="@drawable/rounded_edittext_register"
                android:hint="@string/Choose_password"
                android:paddingLeft="20dp"
                android:textSize="20dp"
                android:textColor="@color/GrayColor"
                android:inputType="textPassword"
                android:layout_alignTop="@+id/student_password_icon"
                android:layout_toRightOf="@+id/student_username_icon"
                android:layout_toEndOf="@+id/student_username_icon" />

            <Button
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:background="@drawable/registro_home_selector"
                android:onClick="goHome"
                android:clickable="true"
                android:layout_centerVertical="true"
                android:layout_alignLeft="@+id/register_panel1"
                android:layout_alignStart="@+id/register_panel1"
                android:layout_marginLeft="10dp"
                />

            <Button
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:background="@drawable/registro_forward_green_selector"
                android:onClick="goNext"
                android:clickable="true"
                android:layout_centerVertical="true"
                android:layout_alignRight="@+id/register_panel1"
                android:layout_alignEnd="@+id/register_panel1"
                android:layout_marginRight="10dp"
                 />

        </RelativeLayout>

        <RelativeLayout
            android:id="@+id/register_page2"
