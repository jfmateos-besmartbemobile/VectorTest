<?xml version="1.0" encoding="utf-8"?>

<com.mobile.android.smartick.UI.RegisterScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/register_horizontalScroll"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:animateLayoutChanges="true"
    android:background="@drawable/intro_page6_bg_sky"
    android:fillViewport="true"
    android:orientation="horizontal"
    android:overScrollMode="never"
    android:scrollbars="none"
    >

  <LinearLayout
      android:id="@+id/register_horizontalScroll_linear"
      android:layout_width="900dp"
      android:layout_height="450dp"
      android:layout_alignParentRight="true"
      android:fillViewport="true"
      android:orientation="horizontal"
      >

    <RelativeLayout
        android:id="@+id/register_page1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="center"
        tools:visibility="gone"
        >

      <ImageView
          android:id="@+id/register_panel1"
          android:layout_width="wrap_content"
          android:layout_height="wrap_content"
          android:layout_centerHorizontal="true"
          android:layout_centerVertical="true"
          android:layout_gravity="center"
          android:adjustViewBounds="true"
          android:scaleType="fitCenter"
          android:src="@drawable/register_panel"
          />

      <Button
          android:id="@+id/b_gohome"
          android:layout_width="wrap_content"
          android:layout_height="wrap_content"
          android:layout_alignLeft="@+id/register_panel1"
          android:layout_alignStart="@+id/register_panel1"
          android:layout_centerVertical="true"
          android:layout_gravity="center"
          android:layout_marginLeft="10dp"
          android:background="@drawable/registro_home_selector"
          android:clickable="true"
          android:onClick="goHome"
          />


      <TextView
          android:id="@+id/student_credentials_title"
          android:layout_width="wrap_content"
          android:layout_height="wrap_content"
          android:layout_alignTop="@+id/register_panel1"
          android:layout_centerHorizontal="true"
          android:layout_marginTop="20dp"
          android:text="@string/Students_Data"
          android:textColor="@color/BlueColor"
          android:textSize="32dp"
          />

      <ImageView
          android:id="@+id/student_username_icon"
          android:layout_width="wrap_content"
          android:layout_height="wrap_content"
          android:layout_below="@+id/student_credentials_title"
          android:layout_marginLeft="54dp"
          android:layout_marginTop="34dp"
          android:layout_toEndOf="@+id/b_gohome"
          android:layout_toRightOf="@+id/b_gohome"
          android:background="@drawable/student_username_selector"
          />

      <ImageView
          android:id="@+id/student_password_icon"
          android:layout_width="wrap_content"
          android:layout_height="wrap_content"
          android:layout_alignLeft="@+id/student_username_icon"
          android:layout_alignStart="@+id/student_username_icon"
          android:layout_below="@+id/student_username_icon"
          android:layout_marginTop="30dp"
          android:background="@drawable/student_password_selector"
          />

      <EditText
          android:id="@+id/student_alias_edittext"
          android:layout_alignTop="@+id/student_username_icon"
          android:layout_toEndOf="@+id/student_username_icon"
          android:layout_toRightOf="@+id/student_username_icon"
          android:hint="@string/Choose_alias"
          android:inputType="textPersonName"
          android:lines="1"
          style="@style/register_edit_text"
          />

      <EditText
          android:id="@+id/student_password_edittext"
          android:layout_alignTop="@+id/student_password_icon"
          android:layout_toEndOf="@+id/student_username_icon"
          android:layout_toRightOf="@+id/student_username_icon"
          android:hint="@string/Choose_password"
          android:inputType="textPassword"
          style="@style/register_edit_text"
          />

      <Button
          android:layout_width="wrap_content"
          android:layout_height="wrap_content"
          android:layout_alignEnd="@+id/register_panel1"
          android:layout_alignRight="@+id/register_panel1"
          android:layout_centerVertical="true"
          android:layout_gravity="center"
          android:layout_marginRight="10dp"
          android:background="@drawable/registro_forward_green_selector"
          android:clickable="true"
          android:onClick="goNext"
          />

    </RelativeLayout>

    <RelativeLayout
